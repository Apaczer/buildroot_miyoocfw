 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/configs/miyoo_defconfig b/arch/arm/configs/miyoo_defconfig
index 89454a7e8..afc1a2c72 100644
--- a/arch/arm/configs/miyoo_defconfig
+++ b/arch/arm/configs/miyoo_defconfig
@@ -125,6 +125,7 @@ CONFIG_PWM=y
 CONFIG_PWM_SUNIV=y
 CONFIG_PHY_SUN4I_USB=y
 CONFIG_NVMEM_SUNXI_SID=y
+CONFIG_EXT4_FS=y
 CONFIG_BTRFS_FS=y
 CONFIG_VFAT_FS=y
 CONFIG_TMPFS=y
@@ -134,7 +135,6 @@ CONFIG_KEYS=y
 # CONFIG_CRYPTO_MANAGER_DISABLE_TESTS is not set
 CONFIG_CRYPTO_DRBG_MENU=y
 # CONFIG_CRYPTO_HW is not set
